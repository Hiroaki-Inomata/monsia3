        <property id="secondary-icon-name" disabled="True"/>
        <property id="secondary-icon-pixbuf" disabled="True"/>
        <property id="secondary-icon-sensitive" disabled="True"/>
        <property id="secondary-icon-stock" disabled="True"/>
